{
  "name": "css-trainer-frontend",
  "version": "0.0.1",
  "main": "index.tsx",
  "repository": "https://@github.com/dewolixgit/css-trainer-frontend.git",
  "author": "d.ragozin <dmtryragozin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "rm-public": "rm -rf ./public",
    "rm-source-maps": "find ./public -name *.js.map -type f -delete",
    "dev": "cross-env NODE_ENV=development API_URL=/api/ webpack serve --mode development",
    "build": "yarn rm-public & cross-env NODE_ENV=production webpack --mode production && yarn run rm-source-maps",
    "tsc:check": "tsc --project tsconfig.json",
    "lint": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint --ignore-path .gitignore . --ext .ts,.tsx,.js,.jsx --fix ",
    "stylelint": "stylelint --ignore-path .gitignore \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint --ignore-path .gitignore \"**/*.{css,scss}\" --fix",
    "format": "yarn lint:fix && yarn stylelint:fix",
    "precommit": "yarn tsc:check && lint-staged",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,js}?(x)": [
      "yarn lint",
      "yarn test"
    ],
    "*.{scss,css}": [
      "yarn stylelint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.5",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "core-js": "^3.20.2",
    "framer-motion": "^10.16.16",
    "mobx": "^6.3.12",
    "mobx-react": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.19.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@dr.pogodin/babel-plugin-react-css-modules": "6.9.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@svgr/webpack": "^6.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.43",
    "@types/react-css-modules": "^4.6.4",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^12.1.7",
    "mini-css-extract-plugin": "^2.4.6",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.3",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "react-refresh": "^0.11.0",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-declaration-strict-value": "^1.9.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.13.1"
  }
}
